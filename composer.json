{
    "name": "asantibanez/laravel-eloquent-state-machines",
    "description": "State machines for your Laravel Eloquent models",
    "keywords": [
        "asantibanez",
        "laravel-eloquent-state-machines"
    ],
    "homepage": "https://github.com/asantibanez/laravel-eloquent-state-machines",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrés Santibáñez",
            "email": "santibanez.andres@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0",
        "javoscript/laravel-macroable-models": "^1.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.4",
        "laravel/pint": "^1.16",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Asantibanez\\LaravelEloquentStateMachines\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asantibanez\\LaravelEloquentStateMachines\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asantibanez\\LaravelEloquentStateMachines\\LaravelEloquentStateMachinesServiceProvider"
            ]
        }
    }
}
